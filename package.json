{
  "name": "@prostojs/parser",
  "version": "0.5.5",
  "description": "Parse anything",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "index.js",
    "dist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "release": "node ./scripts/release.js",
    "test": "jest --runInBand",
    "test:cov": "jest --runInBand --coverage",
    "lint": "eslint --ext .ts src/**.ts --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prostojs/parser.git"
  },
  "keywords": [
    "parser",
    "prostojs"
  ],
  "buildOptions": {
    "name": "ProstoParser",
    "formats": [
      "esm-bundler",
      "esm-browser",
      "cjs",
      "global"
    ]
  },
  "dependencies": {
    "@prostojs/tree": "^0.2.2"
  },
  "gitHooks": {
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "author": "Artem Maltsev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prostojs/parser/issues"
  },
  "homepage": "https://github.com/prostojs/parser#readme",
  "devDependencies": {
    "@prostojs/dye": "^0.3.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "conventional-changelog": "^3.1.24",
    "conventional-changelog-cli": "^2.1.1",
    "enquirer": "^2.3.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "jest": "^29.7.0",
    "minimist": "^1.2.6",
    "rollup": "^4.29.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "semver": "^7.3.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "yorkie": "^2.0.0"
  }
}
